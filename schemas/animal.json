{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "default": {},
  "required": [
    "id",
    "type",
    "name",
    "age",
    "time"
  ],
  "additionalProperties": true,
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id schema",
      "default": 0
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The type schema",
      "default": ""
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name schema",
      "default": ""
    },
    "age": {
      "$id": "#/properties/age",
      "type": "string",
      "title": "The age schema",
      "default": ""
    },
    "time": {
      "$id": "#/properties/time",
      "type": "string",
      "title": "The time schema",
      "default": ""
    }
  }
}